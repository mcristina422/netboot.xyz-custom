#!ipxe
###
### netboot.xyz-mcristina422 menu 
###

:custom
clear custom_choice
menu This is mcristina422 Boot Menu
item --gap Options
item option_one ${space} Loading xcp-ng 8.1 netinstall ISO

choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:option_one
kernel https://boot.netboot.xyz/memdisk raw iso
initrd http://mirrors.xcp-ng.org/isos/8.1/xcp-ng-8.1.0-2-netinstall.iso
boot || goto custom_exit

:custom_exit
chain utils.ipxe
exit
